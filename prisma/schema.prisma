generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           String   @id @default(cuid())
  fullName     String
  phoneNumber  String
  email        String   @unique
  password     String
  companyName  String?
  isAgency     Boolean

  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
